<div id="current-market">
    <table id="share-data">
        <tr>
            <th>SYMBOL</th>
            <th>OPEN</th>
            <th>HIGH</th>
            <th>LOW</th>
            <th>CLOSE</th>
            <th>VOLUME</th>
            <th>%CHANGE</th>

        </tr>
        {{#each .}}
        <tr>
            <td>{{Symbol}} </td>
            <td>{{Open}}</td>
            <td>{{DayHigh}}</td>
            <td>{{DayLow}}</td>
            <td>{{Price}}</td>
            <td>{{Volume}}</td>
            <td>{{ChangePercent}}</td>
        </tr>
        <tr class="fundamental" id="{{Symbol}}">
            <td colspan="7">{{Beta}}</td>
        </tr>
        {{/each}}
    </table>
</div>